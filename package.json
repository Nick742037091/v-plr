{
  "name": "v-plr",
  "version": "0.2.5",
  "license": "MIT",
  "author": "nick h",
  "main": "lib/vue-player.common.js",
  "keyword": "vue video audio player",
  "description": "v-pr",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Nick742037091/v-plr.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:example": "vue-cli-service build",
    "build": "vue-cli-service build --target lib --name vue-player --dest lib --formats commonjs src/index.js",
    "lint": "vue-cli-service lint",
    "test:unit": "cross-env BABEL_ENV=test karma start test/unit/karma.unit.conf.js --single-run",
    "coveralls": "cat test/unit/coverage/lcov.info | ./node_modules/.bin/coveralls",
    "pub": "bash ./build/git-release.sh && bash ./build/publish.sh",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "v-plr": "^0.1.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "coveralls": "^3.0.6",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "epic-spinners": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.7",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^9.4.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vconsole": "^3.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {}
}
